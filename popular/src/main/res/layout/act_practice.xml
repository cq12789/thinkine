<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:fitsSystemWindows="true"
	android:orientation="vertical"
	>
	
	<LinearLayout
		android:id="@+id/ll_count"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical"
		>
		
		<Button
			android:id="@+id/btn_eventbus"
			android:layout_width="match_parent"
			android:layout_height="40dp"
			android:text="eventbus"
			/>
		
		<Button
			android:id="@+id/btn_pop"
			android:layout_width="match_parent"
			android:layout_height="40dp"
			android:text="pop"
			/>
		
		<Button
			android:id="@+id/btn_yuan"
			android:layout_width="match_parent"
			android:layout_height="40dp"
			android:text="圆"
			/>
		
		<Button
			android:id="@+id/btn_select_tp"
			android:layout_width="match_parent"
			android:layout_height="40dp"
			android:text="图片选择"
			/>
		
		<Button
			android:id="@+id/btn_lock"
			android:layout_width="match_parent"
			android:layout_height="40dp"
			android:text="手势锁"
			/>
		
		<Button
			android:id="@+id/btn_quanxian"
			android:layout_width="match_parent"
			android:layout_height="40dp"
			android:text="权限"
			/>
		
		<Button
			android:id="@+id/btn_hd"
			android:layout_width="match_parent"
			android:layout_height="40dp"
			android:text="滑动"
			/>
		
		<Button
			android:id="@+id/btn_zxing"
			android:layout_width="match_parent"
			android:layout_height="40dp"
			android:text="zxing"
			/>
		
		<Button
			android:id="@+id/btn_dialog"
			android:layout_width="match_parent"
			android:layout_height="40dp"
			android:text="dialogplus 全局对话框"
			/>
		
		<Button
			android:id="@+id/btn_mmkv"
			android:layout_width="match_parent"
			android:layout_height="40dp"
			android:text="mmkv替代sp"
			/>
		
		<Button
			android:id="@+id/btn_fingerprint"
			android:layout_width="match_parent"
			android:layout_height="40dp"
			android:text="指纹识别"
			/>
		
		<Button
			android:id="@+id/btn_richeditor"
			android:layout_width="match_parent"
			android:layout_height="40dp"
			android:text="富文本编辑"
			/>
		<Button
			android:id="@+id/btn_toast"
			android:layout_width="match_parent"
			android:layout_height="40dp"
			android:text="toast"
			/>
	</LinearLayout>
</android.support.v4.widget.NestedScrollView>